{#
/**
 * @file
 * Default theme implementation to display a two-column layout.
 *
 * Available variables:
 * - content: The content for this layout.
 * - attributes: HTML attributes for the layout <div>.
 *
 * @ingroup themeable
 */
#}
{% extends 'layout--twocol--node-web-project.html.twig' %}
{% block col_first %}
  {% if content.first %}
    <div {{ region_attributes.first.addClass('layout__region', 'layout__region--first', 'col-12 col-lg-7 align-self-center col-image') }}>
      {{ content.first }}
    </div>
  {% endif %}
{% endblock %}
{% block col_second %}
  {% if content.second %}
    <div {{ region_attributes.second.addClass('layout__region', 'layout__region--second', 'col-12 col-lg-5 col-details') }}>
      {{ content.second }}
    </div>
  {% endif %}
{% endblock %}
